    "context": [
      {
        "pin": 7,
        "displays": "1,2,3/4,5",
        "connect": [
          {
            "method": "PUT",
            "url": "http://ITB-1101-DSP1.byu.edu:8888/sendevent",
            "body": {}
          }
        ]
      }
    ]
  },
  {
    "name": "example job",
    "triggers": [
      {
        "type": "daily",
        "at": "14:45:00-06:00"
      },
      {
        "type": "interval",
        "every": "5m"
      },
      {
        "type": "match",
        "match": {
          "hostname": "teton",
          "event": {
            "eventInfoKey": "button.*",
            "type": "CORE"
          }
        }
      },
      {
        "type": "match"
      }
    ],
    "enabled": false
  }
